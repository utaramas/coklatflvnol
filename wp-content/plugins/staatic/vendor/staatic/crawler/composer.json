{
    "name": "staatic/crawler",
    "type": "library",
    "description": "Staatic allows you to generate a highly optimized static version of your site improving performance as well as security",
    "keywords": [
        "staatic",
        "static site",
        "generator",
        "optimizer"
    ],
    "homepage": "https://staatic.com",
    "license": "proprietary",
    "authors": [
        {
            "name": "Staatic",
            "email": "contact@staatic.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "guzzlehttp/guzzle": "^7.3",
        "guzzlehttp/psr7": "^2.0",
        "psr/log": "^1.0",
        "ramsey/uuid": "^4.2",
        "voku/simple_html_dom": "^4.7"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.0.x-dev"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "autoload": {
        "psr-4": {
            "Staatic\\Crawler\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Staatic\\Crawler\\Tests\\": "tests/"
        }
    }
}
